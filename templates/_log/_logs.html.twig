{% for log in logs %}
    <tr>
        <td headers="logs-version"
            class="text-center col-sm-1 row{{ log.version }}">{{ log.version }}</td>
        <td headers="logs-action"
            class="text-center col-sm-1 row{{ log.version }}">
            <span class="label label-default">{{ ('entity.log.actions.' ~ log.action) | trans }}</span>
        </td>
        <td headers="logs-date"
            class="text-center col-sm-2 row{{ log.version }}">{{ log.loggedAt | localizeddate('medium','medium') }}</td>
        <td headers="logs-user"
            class="text-center col-sm-2 row{{ log.version }}">{{ log.username }}</td>
        <td headers="logs-value" class="row{{ log.version }}">
            {% block addressLog %}
                {% include '_log/_data.html.twig' with {'data': log.data, 'creating': log.action == 'create'} %}
            {% endblock addressLog %}
        </td>
    </tr>
{% endfor %}
